  {
    "name": "header section",
    "settings": [
      {
      "type": "image_picker",
      "id": "logo",
      "label": "painting-image-1"
    }
    ],
        "blocks": [
  {
    "type": "text",
    "name": " COLLECTIONS",
   "settings": [
        {
        
        "type": "text",
        "id": "catagary_text",
        "label": "text"
        },
       {
      "type": "image_picker",
      "id": "icons",
      "label": "painting-image-1"
    }
   ]
  }
  ]
  }
{% endschema %}

