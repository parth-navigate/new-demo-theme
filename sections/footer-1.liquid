<div class="header-1main">
  <div class="parant">
    <img class="footer1logo" src="{{ section.settings.footer1logo | image_url }} "/>
    <div>
                 <ul class="dropdwonmobilemenu">
  {% for link in linklists.main-menu.links %}
    {% assign child_list_handle = link.title | handleize %}
    {% if linklists[child_list_handle].links != blank %}
        <li class="list-style active">
          <a class="linktext" href="{{ link.url }}">{{ link.title }}</a>
          <img class="addicon" src="{{ section.settings.addicon | image_url }}"/>
         <ul class="grandclidehideshow">
           {% for childlink in linklists[child_list_handle].links %}
               <li class="linktext" ><a class="linktext-2" href="{{ childlink.url }}">{{ childlink.title | escape }}</a></li>
            
          {% endfor %}
         </ul>
        </li>
        {% endif %}
    {% endfor %}
                 </ul>
    </div>
  </div>
</div>
{{ 'footer-1-css.css' | asset_url |stylesheet_tag }}
{% schema %}
  {
    "name": "Section name",
    "settings": [
        {
      "type": "image_picker",
      "id": "footer1logo",
      "label": "footer-1 logo"
    }
    ]
  }
{% endschema %}
